<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
	<!-- Servlets -->
	<servlet>
		<servlet-name>ListenProfService</servlet-name>
		<servlet-class>com.ihm.project.app.server.ListenProfServiceImpl</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ListenProfService</servlet-name>
		<url-pattern>/NoteApp/listenProf</url-pattern>
	</servlet-mapping>

	<!-- Default page to serve -->
	<welcome-file-list>
		<welcome-file>NoteApp.html</welcome-file>
	</welcome-file-list>


</web-app>
